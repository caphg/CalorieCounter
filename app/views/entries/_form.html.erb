<div class="form-inline">
  <div class="form-group">
    <label for="meal">Meal</label><br />
    <input type="text" placeholder="Your meal..." ng-model="newEntry.meal">
  </div>
  <div class="form-group">
    <label for="calories">Calories</label><br />
    <input type="number" placeholder="Calories" ng-model="newEntry.calories">
  </div>
  <div class="form-group">
    <label for="date">Date</label><br />
    <input id='newEntryDate' type='text' ng-model="newEntry.date" />
  </div>
  <div class="form-group">
    <label for="description">Description</label><br />
    <input type="text" placeholder="Description..." ng-model="newEntry.description">
  </div>

  <div class="form-group">
    <br />
    <button class="btn btn-primary" ng-click="createNewEntry()">Create</button>
  </div>
</div>